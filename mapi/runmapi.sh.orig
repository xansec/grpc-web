#!/bin/bash

mapi run forallsecure/grpc-web-echo-example \
10 \
--url http://localhost:8080/ \
"net/grpc/gateway/examples/echo/echo.swagger.json" \
--har out.har \
--header "Host: localhost:8080" \
--header "Connection: keep-alive" \
--header "Content-Length: 16" \
--header "sec-ch-ua: \" Not A;Brand\";v=\"99\", \"Chromium\";v=\"99\"" \
--header "X-User-Agent: grpc-web-javascript/0.1" \
--header "sec-ch-ua-mobile: ?0" \
--header "custom-header-1: value1" \
--header "User-Agent: Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/99.0.4844.51 Safari/537.36" \
--header "Content-Type: application/grpc-web-text" \
--header "Accept: application/grpc-web-text" \
--header "X-Grpc-Web: 1" \
--header "sec-ch-ua-platform: \"Linux\"" \
--header "Origin: http://localhost:8081" \
--header "Sec-Fetch-Site: same-site" \
--header "Sec-Fetch-Mode: cors" \
--header "Sec-Fetch-Dest: empty" \
--header "Referer: http://localhost:8081/" \
--header "Accept-Encoding: gzip, deflate, br" \
--header "Accept-Language: en-US,en;q=0.9" $@
